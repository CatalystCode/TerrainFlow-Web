<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <system.webServer>
    <handlers>
      <add name="httpPlatformHandler" path="*" verb="*" modules="httpPlatformHandler" resourceType="Unspecified" />
    </handlers>
    <httpPlatform processPath="%DNX_PATH%" arguments="%DNX_ARGS%" stdoutLogEnabled="true" startupTimeLimit="3600" forwardWindowsAuthToken="true" />
    <security>
      <requestFiltering>
        <requestLimits maxAllowedContentLength="128000000" />
      </requestFiltering>
    </security>
  </system.webServer>
</configuration>